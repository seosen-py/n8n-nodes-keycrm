{
	"name": "n8n-nodes-keycrm",
	"version": "0.1.1",
	"description": "n8n community nodes for KeyCRM API",
	"license": "MIT",
	"homepage": "https://github.com/seosen-py/n8n-nodes-keycrm",
	"bugs": {
		"url": "https://github.com/seosen-py/n8n-nodes-keycrm/issues"
	},
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"keycrm",
		"crm",
		"automation"
	],
	"author": {
		"name": "seosenpy",
		"email": "seo.orchid.kid@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/seosen-py/n8n-nodes-keycrm.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "npm run lint && npm run build"
	},
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/KeycrmApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Keycrm/Keycrm.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
